{
	"name": "tf2dle",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"db:client": "pnpm dlx prisma generate",
		"db:migrate": "pnpm dlx prisma migrate deploy",
		"db:up": "docker compose -f docker-compose.dev.yaml up",
		"db:studio": "pnpm dlx prisma studio",
		"test": "npm run test:integration && npm run test:unit",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"lint": "prettier --check .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@playwright/test": "^1.55.0",
		"@sveltejs/adapter-node": "^5.3.1",
		"@sveltejs/kit": "^2.37.0",
		"@sveltejs/vite-plugin-svelte": "^3.1.2",
		"@testing-library/svelte": "^4.2.3",
		"autoprefixer": "^10.4.21",
		"bits-ui": "^0.15.1",
		"jsdom": "^24.1.3",
		"postcss": "^8.5.6",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prisma": "6.14.0",
		"svelte": "^4.2.20",
		"svelte-check": "^3.8.6",
		"tailwindcss": "^3.4.17",
		"tslib": "^2.8.1",
		"typescript": "^5.9.2",
		"vite": "^5.4.19",
		"vitest": "^1.6.1"
	},
	"type": "module",
	"dependencies": {
		"@oslojs/crypto": "^1.0.1",
		"@prisma/client": "^6.15.0",
		"@types/node-schedule": "^2.1.8",
		"clsx": "^2.1.1",
		"cmdk-sv": "^0.0.13",
		"dayjs": "^1.11.18",
		"lucide-svelte": "^0.314.0",
		"mode-watcher": "^0.1.2",
		"node-schedule": "^2.1.1",
		"svelte-sonner": "^0.3.28",
		"tailwind-merge": "^2.6.0",
		"tailwind-variants": "^0.1.20"
	}
}
