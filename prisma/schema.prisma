datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model DailyMaps {
  id           Int      @id @default(autoincrement())
  selectedAt   DateTime @default(now()) @db.Date
  name         String
  startingPosX Float
  startingPosY Float
  hasWon       Int      @default(0)
}

model Logs {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  event     String
  message   String
}

model DailyWeapons {
  id         Int      @id @default(autoincrement())
  selectedAt DateTime @default(now()) @db.Date
  name       String
  hasWon     Int      @default(0)
}
