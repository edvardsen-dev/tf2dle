datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model DailyMaps {
  id           Int      @id @default(autoincrement())
  selectedAt   DateTime @default(now()) @db.Date
  name         String
  image        String
  startingPosX Float
  startingPosY Float
  gameModes    String
  releaseDate  DateTime @db.Date
  hasWon       Int      @default(0)
}

model Logs {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  event     String
  message   String
}
